{
	"name": "Existing Docker Compose (Extend)",

	"dockerComposeFile": [
		"../docker-compose.yml",
		"docker-compose.yml"
	],

	"customizations": {
		"vscode": {
			"extensions": [
				"ms-toolsai.jupyter",
				"ms-python.python",
				"ms-python.flake8",
				"ms-python.autopep8",
				"GitHub.copilot",
				"oderwat.indent-rainbow"
			]
		}
	},

	"service": "notebook",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}"
}
